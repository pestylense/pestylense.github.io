---
layout: nil
---

<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/atom">
	<channel>
		<title>{{ site.title }}</title>
		<description>{{ site.description }}</description>
		<link>http://pestylense.github.com</link>
		<atom:link href="http://pestylense.github.com/rss.xml" rel="self" type="application/rss+xml" />
		<language>en-us</language>
		<pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
		<lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

		{% for post in site.posts %}
		<item>
			<title>{{ post.title }}</title>
			<link>http://pestylense.github.com{{ post.url }}</link>
			<pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
			<author>pestylense@poniverse.net (Devon Wells)</author>
			<guid>http://mendsley.github.com{{ post.id }}</guid>
			<description>{{ post.content | xml_escape }}</description>
		</item>
		{% endfor %}
	</channel>
</rss>
